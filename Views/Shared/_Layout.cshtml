            background: white;
            margin-top: 40px !important;
            margin-bottom: 50px !important;
        }
        .my-sm-0 {
            height: 55px !important;
            line-height: 1 !important;
            border-radius: 4px;
        }
        .buttonGroup1 {
            position: absolute;
            right: 0.5%;
            display: flex;
        }
        .buttonGroup1Buttons {
            height: 3em;
            min-width: 10em;
        }
        .searchForm {
            max-width: 45% !important;
            display: inline-block !important;
        }
        .acceptedPaymentIconGroup1 {
            line-height: 3;
            text-align: center;
        }
        .acceptedPaymentIconGroup2 {
            text-align: center;
            margin-top: -25px;
        }
        #searchBar {
            max-width: 95% !important;
        }
        .btn-gold {
            min-height: 45px;
            min-width: 10em;
            padding: 15px !important;
            margin-right: 5px;
            margin-bottom: 5px;
            color: #fff !important;
            background-color: #9E7107 !important;
            border-color: #fff !important;
        }
        .btn-gold2 {
            height: 55px;
            width: 4em;
            @* position: absolute; *@
            @* right: 15.5%; *@
            margin-right: 5px;
            margin-bottom: 5px;
            color: #fff !important;
            background-color: #9E7107 !important;
            border-color: #fff !important;
        }
        .btn-danger {
            min-height: 45px;
            width: 10em;
            margin: auto !important;
            padding: 15px !important;
            border-width: 1px !important;
            border-color: #fff !important;
        }
        .btn-info {
            min-height: 45px;
            padding: 15px !important;
            border-width: 1px !important;
            border-color: #fff !important;
        }
        .fa-shopping-cart {
            color: #fff;
        }
        .img-logo {
            height: 110px !important;
            position: absolute;
            top: 14%;
            left: 48%;
        }
        a {
            font-size: 1.1rem;
            color: #343a40;
        }

        a.cart:hover {
            text-decoration: none;
            color: #d60e96;
        }

        a.cart .cart-basket {
            font-size: 1rem;
            position: absolute;
            top: -5px;
            right: -8px;
            width: 22px;
            height: 22px;
            color: #fff;
            background-color: #418deb;
            border-radius: 50%;
        }
        .card {
            background-color: black !important;
            color: #9E7107 !important;
            border: 1px solid !important;
            max-width: 50%;
            margin-right: auto;
            margin-left: auto;
            margin-bottom: 5px;
            text-align: center;
        }
        .footerBar {
            margin-top: 50px;
        }
        button {
            outline: none !important;
        }
        input {
            outline: none !important;
        }
        .cart1 {
            display: block !important;
        }
        .cart2 {
            border: none !important;
            display: none !important;
        }
        @@media (max-width: 991px){
            body {
                padding-top: 150px;
            }
            .img-logo {
                height: 40px !important;
                left: inherit;
                top: 48%;
                right: 3%;
            }
            .navbar {
                min-height: 6.5em !important;
            }
            .card {
                max-width: 90%;
            }
            .login {
                width: fit-content;
                margin: auto !important;
                position: relative !important;
                right: -1px;
            }
            #login-content {
                right: -50px;
            }
            #login-content:after {
                left: 50% !important;
            }
            .btn-gold2 {
                margin: auto;
                margin-bottom: 5px;
            }
            .buttonGroup1 {
                position: relative;
                display: block;
                right: 3px;
            }
            .buttonGroup1Buttons {
                margin: auto;
            }
            .dangerButton1 {
                margin-left: 4px;
            }
            .cart1 {
                display: none !important;
            }
            .cart2 {
                display: block !important;
            }
