    <title>@ViewData["Title"] Inflate LLC</title>
    <style>
        body {
            padding-top: 200px;
            margin-bottom: 0px !important;
        }
        .navbar-brand {
            margin-left: 2%;
            color: black !important;
        }
        .nav-link {
            white-space: nowrap;
            color: black !important;
        }
        .navbar-toggler {
            border: 0px !important;
            position: absolute;
            top: 17px;
            left: 0;
        }
        .navbar-toggler-icon {
            margin-top: 115% !important;
        }
        .OpO {
            height: 25px;
            margin-top: 8px;
            margin-right: 15px;
            padding-right: 25px !important;
            @* border-right: 1px solid white; *@
            line-height: 8px;
        }
        #OaO {
            width: 98%
        }
        .navbar-brand {
            font-size: 25px !important;
            margin-top: -5px;
            margin-right: 30px !important;
        }
        .img15 {
            height: 90px;
        }
        .searchButton {
            margin-left: -7px !important;
        }
        .nav-link {
            color: #fff !important;
        }
        #ul1 {
        margin: 0;
        padding: 0;
        list-style: none;
        position: relative;
        float: right;
        }
        .login {
            @* position: absolute !important; *@
            right: 0.5%;
            margin-top: 0 !important;
            margin-right: 5px !important;
        }
        #login-content {
            width: 16.6em;
            margin-top: 40px;
            display: none;
            position: absolute;
            top: 24px;
            right: 0;
            z-index: 999;
            background: #fff;
            padding: 15px;
        }
            #login-content:after, #login-content:before {
            bottom: 100%;
            left: 83%;
            border: solid transparent;
            content: " ";
            @* height: 0;
            width: 0; *@
            position: absolute;
            pointer-events: none;
        }

        #login-content:after {
            border-bottom-color: #fff;
            border-width: 10px;
        }
        
        #inputs input {
            background: hsla(0, 0%, 100%, 0.9);
            color: #606162;
            padding: 6px 5px;
            margin: 0 0 10px 0;
            width: 238px;
            border: 1px solid rgba(57, 59, 61, .2);
            border-radius: 8px;
        }

        #inputs input:focus {
            background: hsla(0, 0%, 100%, 0.9);
            border-color: rgba(0, 0, 0, 0.3);
            outline: none;
            box-shadow: 0 0 0 1px #e8c291 inset;
        }

        #login #actions {
            @* margin: 10px 0 0 0; *@
        }

        #submitLogin {
            height: 30px;
            width: 238px;
            background-color: transparent;
            border-radius: 8px;
            color: #393b3d;
            float: left;
            padding: 0;
            cursor: auto;
            font: 14px Arial, Helvetica;
            font-weight: 300;
            border: 1px solid gray;
        }
        
        #submitLogin:hover,
        #submitLogin:focus {
            border: 1px solid #393b3d;
        }

        #submitLogin:active {
            outline: none;
            box-shadow: 0 1px 4px rgba(0, 0, 0, 0.5) inset;
        }

        #label {
            float: right;
            line-height: 30px;
        }

        #label input {
            position: relative;
            top: 2px;
            right: 2px;
        }
        #register {
            display: block;
            text-align: center;
            text-decoration: underline;
            margin-top: 35px;
            margin-bottom: -7px;
            color: black;
        }
        ::-webkit-input-placeholder { /* Chrome/Opera/Safari */
                color: rgba(75,149,200,0.7);
        }
        ::-moz-placeholder { /* Firefox 19+ */
            color: rgba(75,149,200,0.7);
        }
        :-ms-input-placeholder { /* IE 10+ */
            color: rgba(75,149,200,0.7);
        }
        :-moz-placeholder { /* Firefox 18- */
            color: rgba(75,149,200,0.7);
        }
        .pink {
            color: #B98389;
        }
        #topBar {
            width: 100%;
            margin-left: -17px;
            z-index: 4;
            top: -30px;
            position: absolute;
            background-color: black;
            padding: 2px;
            padding-left: 15px;
        }
        .sideBarL {
            height: 100%;
            width: 5%;
            @* z-index: 2; *@
            max-width: 10px;
            background-color: #9E7107;
            top: 0;
            bottom: 0;
            left: 0;
            position:fixed;
        }
        .sideBarR {
            height: 100%;
            width: 5%;
            @* z-index: 2; *@
            max-width: 10px;
            background-color: #9E7107;
            top: 0;
            bottom: 0;
            right: 0;
            position:fixed;
        }
        body {
            background-color: black !important;
        }
        .active {
            margin: auto;
        }
        .navbar-nav {
            text-align: center;
        }
        .socialLinks {
            margin-right: 2px;
            color: gray !important;
        }
